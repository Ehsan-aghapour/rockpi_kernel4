			rockchip,pins =
				<2 RK_PD3 0 &pcfg_pull_none>;
		};
	};

	host_5v {
		usb1_en_oc: usb1-en-oc {
			rockchip,pins =
				<4 25 RK_FUNC_GPIO &pcfg_pull_up_10ma>;
		};
	};

	vbus_typec {
		usb0_en_oc: usb0-en-oc {
			rockchip,pins =
				<4 RK_PD2 RK_FUNC_GPIO &pcfg_pull_up_10ma>;
		};
	};

